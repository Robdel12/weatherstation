    "pretender": "^3.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "recharts": "^2.1.6",
    "tailwindcss": "^1.9.6",
    "testing-hooks": "^0.2.0",
    "urql": "^2.0.5",
    "@swc/helpers": "^0.2.13"
