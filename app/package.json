{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "description": "React SPA to display weather station data",
  "license": "MIT",
  "scripts": {
    "start": "parcel watch src/index.html",
    "test": "start-server-and-test 'yarn workspace server start' 8080 'cypress run'",
    "lint": "eslint --ignore-path ../.gitignore .",
    "test:percy": "percy exec -- yarn test --single-run",
    "build": "NODE_ENV=production parcel build src/index.html"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "default": "dist/index.html",
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "dayjs": "^1.10.7",
    "graphql": "^16.1.0",
    "parcel": "^2.0.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "recharts": "^2.1.6",
    "tailwindcss": "^3.0.7",
    "urql": "^2.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^9.1.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.27.1",
    "start-server-and-test": "^1.14.0"
  }
}
