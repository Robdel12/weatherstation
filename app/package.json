{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "description": "React SPA to display weather station data",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html",
    "test": "NODE_ENV=test karma start",
    "lint": "eslint --ignore-path ../.gitignore .",
