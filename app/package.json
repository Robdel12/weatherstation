{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "description": "React SPA to display weather station data",
  "license": "MIT",
  "scripts": {
    "start": "parcel watch src/index.html",
    "test": "start-server-and-test 'yarn workspace server start' 8080 'cypress run'",
    "lint": "eslint --ignore-path ../.gitignore .",
    "test:percy": "percy exec -- yarn test --single-run",
    "build": "NODE_ENV=production parcel build src/index.html"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "default": "dist/index.html",
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.13",
    "dayjs": "^1.11.6",
    "graphql": "^16.6.0",
    "parcel": "^2.8.0",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.1",
    "postcss-preset-env": "^7.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "recharts": "^2.1.12",
    "tailwindcss": "^3.2.4",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^9.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.31.11",
    "start-server-and-test": "^1.14.0"
  }
}
