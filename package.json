  "dependencies": {
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "mongodb": "^3.1.10"
  },
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
