{
  "name": "weather-station",
  "version": "0.1.0",
  "description": "Node.js server backed by MongoDB to collect weather station data & React frontend SPA",
  "main": "src/server/index.js",
  "author": "Robert DeLuca",
  "license": "MIT",
  "scripts": {
    "start:server": "TZ=\"America/Chicago\" node ./src/server/index.js",
    "start": "TZ=\"America/Chicago\" node ./src/server/index.js",
    "start:server:debug": "nodemon --inspect ./src/server/index.js",
    "start:client": "webpack --watch",
    "start:all": "yarn start:client & yarn start:server:debug",
    "start:test": " yarn build:client && yarn start:server",
    "build:client": "NODE_ENV=production webpack",
    "postinstall": "yarn build:client",
    "test": "NODE_ENV=test karma start",
    "test:percy": "NODE_ENV=test percy exec -- karma start --single-run"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongodb": "^3.1.10",
    "node-github-graphql": "^0.2.7",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@interactor/percy": "^0.2.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "dayjs": "^1.8.8",
    "html-webpack-plugin": "^3.2.0",
    "interactor.js": "^1.4.4",
    "karma": "^4.1.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.9",
    "pretender": "^3.0.1",
    "react": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.23.1",
    "testing-hooks": "^0.1.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "engines": {
    "node": "10"
  }
}
