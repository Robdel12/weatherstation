{
  "name": "weather-station-server",
  "version": "0.1.0",
  "description": "Node.js server backed by MongoDB to collect weather station data",
  "main": "src/index.js",
  "author": "Robert DeLuca",
  "license": "MIT",
  "scripts": {
    "start": "TZ=\"America/Chicago\" node ./src/index.js",
