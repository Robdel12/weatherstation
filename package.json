{
  "name": "weather-station",
  "version": "0.1.0",
  "description": "Node.js server backed by MongoDB to collect weather station data & React frontend SPA",
  "main": "src/server/index.js",
  "author": "Robert DeLuca",
  "license": "MIT",
  "scripts": {
    "start:server": "TZ=\"America/Chicago\" node ./src/server/index.js",
    "start": "TZ=\"America/Chicago\" node ./src/server/index.js",
    "start:server:debug": "nodemon --inspect ./src/server/index.js",
    "start:client": "webpack --watch",
    "start:all": "yarn start:client & yarn start:server:debug",
    "start:test": " yarn build:client && yarn start:server",
    "build:client": "NODE_ENV=production webpack",
    "postinstall": "yarn build:client",
    "test": "NODE_ENV=test karma start",
    "test:percy": "NODE_ENV=test percy exec -- karma start --single-run"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongodb": "^3.4.1",
    "node-github-graphql": "^0.2.7",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/polyfill": "^7.8.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@interactor/percy": "^0.2.2",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "dayjs": "^1.8.19",
    "html-webpack-plugin": "^3.2.0",
    "interactor.js": "^1.6.1",
    "karma": "^4.4.1",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "pretender": "^3.2.0",
    "react": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "^1.1.2",
    "testing-hooks": "^0.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "node": "10"
  }
}
